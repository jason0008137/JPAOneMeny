<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="classicmodels" dynamic-insert="false" dynamic-update="false" mutable="true" name="MyMod.Epproduct" optimistic-lock="none" polymorphism="implicit" select-before-update="false" table="epproduct">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property generated="never" lazy="false" name="name" optimistic-lock="true" type="string" unique="false">
            <column length="45" name="name" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="des" optimistic-lock="true" type="string" unique="false">
            <column length="45" name="des" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="imagepath" optimistic-lock="true" type="string" unique="false">
            <column length="45" name="imagepath" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="price" optimistic-lock="true" type="double" unique="false">
            <column name="price" not-null="true" precision="22" scale="0"/>
        </property>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="eporderdts" optimistic-lock="true" sort="unsorted" table="eporderdt">
            <key on-delete="noaction">
                <column name="pid" not-null="true"/>
            </key>
            <one-to-many class="MyMod.Eporderdt" embed-xml="true" not-found="exception"/>
        </set>
    </class>
</hibernate-mapping>
