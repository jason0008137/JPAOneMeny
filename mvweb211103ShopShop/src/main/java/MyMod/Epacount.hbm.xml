<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="classicmodels" dynamic-insert="false" dynamic-update="false" mutable="true" name="MyMod.Epacount" optimistic-lock="none" polymorphism="implicit" select-before-update="false" table="epacount">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property generated="never" lazy="false" name="username" optimistic-lock="true" type="string" unique="false">
            <column length="45" name="username" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="pw" optimistic-lock="true" type="string" unique="false">
            <column length="45" name="pw" not-null="true"/>
        </property>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="eporders" optimistic-lock="true" sort="unsorted" table="eporder">
            <key on-delete="noaction">
                <column name="uid" not-null="true"/>
            </key>
            <one-to-many class="MyMod.Eporder" embed-xml="true" not-found="exception"/>
        </set>
    </class>
</hibernate-mapping>
