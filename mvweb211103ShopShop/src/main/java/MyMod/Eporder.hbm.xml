<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="classicmodels" dynamic-insert="false" dynamic-update="false" mutable="true" name="MyMod.Eporder" optimistic-lock="none" polymorphism="implicit" select-before-update="false" table="eporder">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <many-to-one class="MyMod.Epacount" embed-xml="true" fetch="select" insert="true" name="epacount" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="uid" not-null="true"/>
        </many-to-one>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="eporderdts" optimistic-lock="true" sort="unsorted" table="eporderdt">
            <key on-delete="noaction">
                <column name="oid" not-null="true"/>
            </key>
            <one-to-many class="MyMod.Eporderdt" embed-xml="true" not-found="exception"/>
        </set>
    </class>
</hibernate-mapping>
